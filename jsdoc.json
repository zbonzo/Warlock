{
  "source": {
    "include": ["."],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)node_modules(\\/|\\\\|$)|(^|\\/|\\\\)\\.git(\\/|\\\\|$)|(^|\\/|\\\\)docs(\\/|\\\\|$)"

  },
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "recurse": true, 
    "template": "node_modules/docdash" 
  },
  "plugins": [], 
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}